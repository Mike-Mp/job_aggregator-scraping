extends layout
include ./mixins/_indeedCard
include ./mixins/_stackCard
block content
  a(href="/") Index
  if stackoverflowResult.length !== 0
    div(class="stackoverflow")
      each stackInfo in stackoverflowResult
        +stackCard(stackInfo)
  if indeedResult.length !== 0
    div(class="indeed")
      each indeedInfo in indeedResult
        +indeedCard(indeedInfo)

  
